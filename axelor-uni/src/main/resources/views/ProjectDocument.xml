<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<object-views xmlns="http://axelor.com/xml/ns/object-views"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://axelor.com/xml/ns/object-views http://axelor.com/xml/ns/object-views/object-views_5.3.xsd">
    
    <grid name="project-document-grid" title="Project Documents" model="com.axelor.apps.uni.db.ProjectDocument" >
		<field name="name" />
		<field name="documentFile" />
    </grid>
    
   	<form name="project-document-form" title="Project Document" model="com.axelor.apps.uni.db.ProjectDocument" width="1300">
   	
   	<panel name="firstPanel" colSpan="12">
		<field name="name" />
		<field name="documentFile" widget="binary-link" />
		<field name="discription" widget="html" colSpan="12"/>
	</panel>
	
	</form>
</object-views>
